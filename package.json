{
  "name": "@heinz-95729/project",
  "version": "0.1.0",
  "description": "A simple bookstore built with a VueJS web app and a NodeJS server",
  "private": true,
  "engines": {
    "node": ">=18.14.1"
  },
  "type": "module",
  "bin": {
    "cuid": "./bin/cuid.js"
  },
  "scripts": {
    "dev:server": "cd server; pnpm run dev",
    "dev:client": "cd client; pnpm run dev",
    "dev": "concurrently \"pnpm run dev:*\" --raw --kill-others",
    "build": "cd client; pnpm run build",
    "preview": "cd client; pnpm run preview",
    "check": "cd client; pnpm run check",
    "check:watch": "cd client; pnpm run check:watchc",
    "test:client": "cd client; pnpm test",
    "test:lib": "cd lib; supposed -r nyan",
    "test:lib:live": "nodemon --watch lib -e js --exec 'cd lib; supposed'",
    "test:server": "cd server; supposed -r nyan",
    "test:server:live": "nodemon --watch server -e js --exec 'cd server; supposed'",
    "test": "pnpm run test:client & pnpm run test:lib & pnpm run test:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/losandes/heinz-95729-project.git"
  },
  "keywords": [],
  "author": "losandes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/losandes/heinz-95729-project/issues"
  },
  "homepage": "https://github.com/losandes/heinz-95729-project#readme",
  "devDependencies": {
    "@paralleldrive/cuid2": "^2.2.0",
    "@polyn/logger": "^1.0.2",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.21",
    "tailwindcss": "^3.2.7",
    "test": "npm:supposed@^1.3.3",
    "unexpected": "^13.1.0"
  },
  "dependencies": {
    "@polyn/blueprint": "^2.7.0",
    "@polyn/immutable": "^1.1.2",
    "events": "^3.3.0"
  }
}
