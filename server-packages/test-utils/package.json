{
  "name": "@heinz-95729/test-utils",
  "version": "0.1.0",
  "description": "Test utilities to help with mocks and writing tests",
  "type": "module",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=18.14.2"
  },
  "scripts": {
    "test": "supposed",
    "test:ci": "supposed -r tap,summary --no-color",
    "test:coverage:ci": "c8 --reporter=text --reporter=text-summary pnpm run test:ci",
    "coverage:ci": "mkdir -p ./coverage && touch ./coverage/lcov.info; c8 report --reporter=text-lcov > ./coverage/lcov.info"
  },
  "author": "losandes",
  "license": "UNLICENSED",
  "devDependencies": {
    "@heinz-95729/test-utils": "link:",
    "@paralleldrive/cuid2": "^2.2.0",
    "@polyn/logger": "^1.0.2",
    "c8": "^7.13.0",
    "keyv": "^4.5.2",
    "koa": "^2.14.1",
    "node-mocks-http": "^1.12.1",
    "supposed": "^1.3.3",
    "unexpected": "^13.1.0"
  }
}
